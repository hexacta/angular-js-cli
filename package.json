{
  "name": "angular-js-cli",
  "description": "AngularJS cli with Frontend StackWeb",
  "version": "0.0.1",
  "scripts": {
    "angular": "gulp angular",
    "init": "npm install",
    "install": "bower install && gulp angular-app --first",
    "start": "gulp serve-dev",
    "generate": "gulp generate-angular",
    "generate:app": " bower install && gulp angular-app",
    "dev": "gulp serve-dev",
    "dev:mock": "gulp serve-dev --mocked",
    "prod": "gulp serve-build",
    "prod:mock": "gulp serve-build --mocked",
    "build": "gulp build",
    "start:selenium": "webdriver-manager update && webdriver-manager update --ie && start cmd /C webdriver-manager start",
    "lint": "gulp vet",
    "lint:fix": "gulp vet --fix",
    "analysis": "gulp plato  && start .\\reports\\plato\\index.html",
    "test": "gulp test",
    "test:report": "gulp test && start .\\reports\\unit-coverage\\report-html\\index.html",
    "e2e": "webdriver-manager update && webdriver-manager update --ie && start cmd /C webdriver-manager start && gulp e2e",
    "e2e:report": "webdriver-manager update && webdriver-manager update --ie && start cmd /C webdriver-manager start && gulp e2e && for /r \".\\reports\\e2e\\\" %x in (*.html) do call \"%x\"",
    "e2e:serve": "webdriver-manager update && webdriver-manager update --ie && start cmd /C gulp serve-dev --mocked && start cmd /C webdriver-manager start  && gulp e2e",
    "e2e:serve:prod": "webdriver-manager update && webdriver-manager update --ie && start cmd /C gulp serve-build --mocked && start cmd /C webdriver-manager start  && gulp e2e"
  },
  "devDependencies": {
    "browser-sync": "~2.11.2",
    "chalk": "^1.1.0",
    "del": "^1.2.0",
    "eslint": "^3.19.0",
    "eslint-plugin-angular": "^2.3.0",
    "glob": "^4.5.3",
    "gulp": "^3.8.10",
    "gulp-angular-embed-templates": "^2.3.0",
    "gulp-angular-protractor": "^0.4.2",
    "gulp-bump": "^0.3.1",
    "gulp-eslint": "^3.0.1",
    "gulp-filter": "^2.0.2",
    "gulp-flatten": "^0.3.1",
    "gulp-header": "^1.2.2",
    "gulp-if": "^1.2.5",
    "gulp-imagemin": "^2.3.0",
    "gulp-inject": "^4.2.0",
    "gulp-load-plugins": "^1.0.0-rc.1",
    "gulp-minify-css": "^1.1.1",
    "gulp-minify-html": "^1.0.4",
    "gulp-order": "^1.1.1",
    "gulp-plumber": "^1.0.1",
    "gulp-print": "^1.1.0",
    "gulp-prompt": "^0.2.0",
    "gulp-protractor": "^4.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace-task": "^0.11.0",
    "gulp-rev": "^5.1.0",
    "gulp-rev-replace": "^0.4.2",
    "gulp-sass": "^3.1.0",
    "gulp-task-listing": "^1.0.0",
    "gulp-uglify": "^2.1.0",
    "gulp-useref": "^1.0.2",
    "gulp-util": "^3.0.1",
    "gulp-webserver": "^0.9.1",
    "jasmine-core": "^2.5.2",
    "jasmine-reporters": "^2.2.1",
    "jasmine-spec-reporter": "^3.2.0",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^0.1.3",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-spec-reporter": "0.0.30",
    "lodash": "^3.10.0",
    "node-notifier": "^4.0.3",
    "plato": "git+https://github.com/Lingvokot/es6-plato.git",
    "protractor": "^5.1.1",
    "protractor-jasmine2-html-reporter": "0.0.7",
    "protractor-uisref-locator": "^1.2.0",
    "require-dir": "^0.3.1",
    "webdriver-manager": "^12.0.5",
    "wiredep": "^4.0.0",
    "yargs": "^3.15.0"
  },
  "main": "gulp.config.js",
  "author": "",
  "license": "ISC"
}
